{
	"scripts": {
		"build-swytch": "npx tailwindcss -i ./wp-content/themes/swytch/src/assets/css/style.scss -o ./wp-content/themes/swytch/dist/style.css && npx webpack --config webpack.config.js",
		"dev-swytch": "npx tailwindcss -i ./wp-content/themes/swytch/src/assets/css/style.scss -o ./wp-content/themes/swytch/dist/style.css --watch & npx webpack --watch --config webpack.config.js"
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"babel-loader": "^9.1.2",
		"postcss": "^8.3.5",
		"sass": "^1.35.2",
		"tailwindcss": "^3.2.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	}
}
